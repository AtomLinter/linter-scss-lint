{
  "name": "linter-scss-lint",
  "main": "./lib/init",
  "version": "2.2.2",
  "description": "Lint SCSS on the fly, using scss-lint",
  "repository": "https://github.com/AtomLinter/linter-scss-lint",
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint ./lib"
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "dependencies": {
    "atom-linter": "^3.2.0",
    "atom-package-deps": "^2.0.2"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.1.0": "provideLinter"
      }
    }
  },
  "devDependencies": {
    "coffeelint": "^1.12.1"
  }
}
